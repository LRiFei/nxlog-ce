	<member>Type</member><member><link linkend="lang_type_string">string</link></member>
	<!--<member>Persist</member><member>persist</member>-->
       </simplelist>
	The category name resolved from Task.
      </para>
     </listitem>
   </varlistentry>

   <varlistentry> 
     <term><anchor id="im_msvistalog_field_Keywords"/>$Keywords</term>
